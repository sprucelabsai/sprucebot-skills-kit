    "jest-cli": "^22.0.5",
    "jest-styled-components": "^4.9.0",
    "prettier": "^1.7.4",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.1.4",
    "svgo": "^1.0.3"
