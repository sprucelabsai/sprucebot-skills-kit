    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "config": "^1.27.0",
    "dotenv": "^4.0.0",
    "eslint-plugin-react": "^7.4.0",
    "isomorphic-fetch": "^2.2.1",
