    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "next": "^4.1.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-emojione": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-sprucebot": "^0.10.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.17.2",
